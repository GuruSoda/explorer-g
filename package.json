    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-fileupload": "^1.3.1",
    "morgan": "~1.9.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "concurrently": "^7.0.0"
  }
}
